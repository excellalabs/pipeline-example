{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "app/**/*.js",
    "server/**/*.js",
    "!**/node_modules/**",
    "!**/vendor/**",
    "!**/__tests__/**",
    "!server/api/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 50,
      "branches": 50,
      "functions": 50,
      "lines": 50
    }
  }
}