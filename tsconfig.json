{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "sourceMap": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5"
  },
  "include": [
    "app"
  ],
  "exclude": [
    "node_modules",
    "__tests__"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
